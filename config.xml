    <platform name="ios">
        <icon src="incon.png" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="~1.3.2" />
    <plugin name="cordova-plugin-disable-nsapptransportsecurity" spec="~1.0.2" />
    <plugin name="cordova-plugin-file" spec="~4.3.3" />
    <plugin name="phonegap-plugin-push" spec="~2.0.0-rc5">
        <variable name="SENDER_ID" value="758907412940" />
    </plugin>
    <plugin name="cordova-plugin-camera" spec="~2.4.1" />
    <plugin name="cordova-plugin-compat" spec="~1.1.0" />
